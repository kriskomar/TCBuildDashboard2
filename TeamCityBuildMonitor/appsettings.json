{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TeamCity": {
    "api_url": "{your-tc-base-url-here}",
    "api_projects": "/httpAuth/app/rest/projects",
    "api_buildtypes": "/httpAuth/app/rest/buildTypes",
    "api_buildstatus": "/httpAuth/app/rest/buildTypes/id:{0}/builds/branch:(default:any)",
    "api_runningbuilds": "/httpAuth/app/rest/builds?locator=running:true,branch:(default:any)",
    "api_buildqueue": "/httpAuth/app/rest/buildQueue"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/{0}",
    "Domain": "{your-sharepoint-base-url}.onmicrosoft.com",
    "TenantId": "{tenant-id-here}",
    "ClientId": "{client-id-here}",
    "CallbackPath": "/signin-oidc",
    "AppKey": "{your-app-key-here}",
    "GraphResourceId": "https://graph.microsoft.com/"
  }
}